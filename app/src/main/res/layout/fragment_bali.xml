<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragments.BaliFragment">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="719dp"
        android:layout_height="345dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="120dp"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardviewbali1"
                    android:layout_width="match_parent"
                    android:layout_height="125dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="20dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="15dp"
                        app:srcCompat="@drawable/satel" />

                    <TextView
                        android:layout_width="239dp"
                        android:layout_height="80dp"
                        android:layout_marginLeft="125dp"
                        android:layout_marginTop="40dp"
                        android:text=" SATE LILIT"
                        android:textAlignment="center"
                        android:textSize="30sp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardviewbali2"
                    android:layout_width="match_parent"
                    android:layout_height="125dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="20dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="15dp"
                        app:srcCompat="@drawable/nasibabi" />

                    <TextView
                        android:layout_width="225dp"
                        android:layout_height="80dp"
                        android:layout_marginLeft="125dp"
                        android:layout_marginTop="40dp"
                        android:text=" BABI GULING"
                        android:textAlignment="center"
                        android:textSize="30sp" />
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardviewbali3"
                    android:layout_width="match_parent"
                    android:layout_height="125dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="20dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="15dp"
                        app:srcCompat="@drawable/nasijinggo" />

                    <TextView
                        android:layout_width="225dp"
                        android:layout_height="80dp"
                        android:layout_marginLeft="125dp"
                        android:layout_marginTop="40dp"
                        android:text="NASI JINGGO"
                        android:textAlignment="center"
                        android:textSize="25sp" />
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardviewbali5"
                    android:layout_width="match_parent"
                    android:layout_height="125dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="20dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="15dp"
                        app:srcCompat="@drawable/komoh" />

                    <TextView
                        android:layout_width="225dp"
                        android:layout_height="80dp"
                        android:layout_marginLeft="125dp"
                        android:layout_marginTop="40dp"
                        android:text=" KOMOH"
                        android:textAlignment="center"
                        android:textSize="25sp" />
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardviewthai5"
                    android:layout_width="match_parent"
                    android:layout_height="125dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="20dp">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="15dp"
                        app:srcCompat="@drawable/khao" />

                    <TextView
                        android:layout_width="225dp"
                        android:layout_height="80dp"
                        android:layout_marginLeft="125dp"
                        android:layout_marginTop="40dp"
                        android:text=" KHAO TOM"
                        android:textAlignment="center"
                        android:textSize="25sp" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </ScrollView>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>